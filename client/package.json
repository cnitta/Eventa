{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "dependencies": {
    "@material-ui/core": "1.5.0",
    "@material-ui/icons": "2.0.2",
    "ajv": "6.5.2",
    "bootstrap": "^4.1.3",
    "classnames": "2.2.6",
    "css-loader": "^1.0.0",
    "firebase": "^5.5.7",
    "history": "4.7.2",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "node-sass-chokidar": "1.3.3",
    "npm-run-all": "4.1.3",
    "popper.js": "^1.14.4",
    "prop-types": "15.6.2",
    "react": "^16.5.0",
    "react-cookie": "^3.0.4",
    "react-credit-card-input": "^1.0.15",
    "react-datetime": "^2.16.2",
    "react-dom": "^16.5.0",
    "react-google-maps": "^9.4.5",
    "react-input-range": "^1.3.0",
    "react-moment": "^0.8.3",
    "react-native-web": "^0.9.6",
    "react-nouislider": "2.0.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.0.17",
    "react-scroll-to-component": "^1.0.2",
    "react-slick": "0.23.1",
    "react-swipeable-views": "0.12.16",
    "react-time": "^4.3.0",
    "react-transition-group": "^2.4.0",
    "react-web-gifted-chat": "0.0.4",
    "reactjs-simple-spinner": "0.0.5",
    "reactstrap": "^6.4.0",
    "style-loader": "^0.23.0",
    "sweetalert2": "^7.29.0",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=./src react-scripts start",
    "build:symlinked": "cross-env NODE_PATH=./node_modules browserify src/index.js > dist/build.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "build-package-css": "node-sass-chokidar src/assets/scss/material-kit-react.scss dist/material-kit-react.css",
    "build-package": "npm run build-package-css && babel src --out-dir dist"
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "postcss-loader": "^3.0.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-import-rename": "^1.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1"
  },
  "optionalDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "prettier": "^1.13.7"
  }
}
